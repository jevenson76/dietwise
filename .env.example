# DietWise Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Keys
API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Google AdSense
REACT_APP_ADSENSE_CLIENT_ID=ca-pub-XXXXXXXXXXXXXXXX
REACT_APP_AD_SLOT_FOOD_LOG=XXXXXXXXXX
REACT_APP_AD_SLOT_FOOD_LIBRARY=XXXXXXXXXX
REACT_APP_AD_SLOT_MEAL_IDEAS=XXXXXXXXXX
REACT_APP_AD_SLOT_PROFILE=XXXXXXXXXX
REACT_APP_AD_SLOT_SETTINGS=XXXXXXXXXX
REACT_APP_AD_SLOT_ANALYTICS=XXXXXXXXXX
REACT_APP_ADSENSE_PRODUCTION=false

# BrowserStack Testing
BROWSERSTACK_USERNAME=your_browserstack_username
BROWSERSTACK_ACCESS_KEY=your_browserstack_access_key

# Supabase (if using)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe (if using)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key

# Other services
CODECOV_TOKEN=your_codecov_token
PERCY_TOKEN=your_percy_token