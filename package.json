{
  "name": "dietwise---your-personal-dieting-companion",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build --mode production",
    "build:gh-pages": "vite build --base=/dietwise/",
    "preview": "vite preview",
    "predeploy": "npm run build:gh-pages",
    "deploy": "gh-pages -d dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:mobile": "npm run build && cap sync && npm run test:e2e -- --project='Mobile Chrome' --project='Mobile Safari'",
    "test:a11y": "playwright test tests/e2e/accessibility.spec.ts",
    "test:visual": "playwright test tests/e2e/visual-regression.spec.ts",
    "test:browserstack": "playwright test --config=browserstack.config.ts",
    "validate:browserstack": "node scripts/validate-browserstack.js",
    "playwright:install": "npx playwright install --with-deps",
    "setup:credentials": "node scripts/setup-credentials.cjs",
    "test:credentials": "node scripts/test-credentials.cjs",
    "setup:test-env": "node scripts/setup-test-env.cjs",
    "enter:credentials": "node scripts/enter-credentials.cjs",
    "optimize:rls": "node scripts/apply-rls-optimization.cjs",
    "setup:production": "node scripts/production-setup.cjs",
    "audit:security": "node scripts/audit-security.cjs",
    "validate-credentials": "node scripts/validate-credentials.js",
    "optimize:images": "node scripts/optimize-images.js",
    "analyze:bundle": "npm run build && open dist/stats.html",
    "validate:credentials": "node scripts/validate-credentials.js",
    "validate": "node scripts/validate-credentials.js"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.0",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/cli": "^7.4.0",
    "@capacitor/core": "^7.4.0",
    "@capacitor/ios": "^7.4.0",
    "@capacitor/local-notifications": "^7.0.1",
    "@capacitor/push-notifications": "^7.0.1",
    "@google/genai": "^1.4.0",
    "@sentry/react": "^9.30.0",
    "@sentry/vite-plugin": "^3.5.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@zxing/library": "^0.21.3",
    "capacitor-biometric-auth": "^0.1.1",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^22.14.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "browserstack-local": "^1.5.7",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.1.1",
    "happy-dom": "^18.0.1",
    "imagemin": "^9.0.1",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
